{
  "name": "@fickou/adonis-access-control-list",
  "version": "1.0.6",
  "description": "Help dev to filter database with query parameters ",
  "main": "index.js",
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "lint": "eslint . --ext=.ts --fix",
    "lint:clean": "prettier --write ./src ./providers ./adonis-typings",
    "pretest": "npm run lint",
    "test": "node -r @adonisjs/require-ts/build/register japaFile.ts",
    "test:nyc": "nyc npm run test",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "clean": "del-cli build",
    "compile": "npm run lint && npm run clean && tsc && npm run copy:files",
    "build": "npm run compile",
    "copy:files": "copyfiles 'instructions.md' build && copyfiles 'ressources/**/*.edge' build && copyfiles 'templates/**/*.txt' build",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afidosstar/adonis-access-control-list.git"
  },
  "keywords": [
    "adonisjs",
    "filter",
    "search",
    "helper",
    "database"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "author": "Fiacre Dossou AYEDOUN",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/afidosstar/adonis-access-control-list/issues"
  },
  "homepage": "https://github.com/afidosstar/adonis-access-control-list#readme",
  "devDependencies": {
    "@adonisjs/auth": "^8.2.2",
    "@adonisjs/http-server": "^5.11.0",
    "@adonisjs/lucid": "^18.2.0",
    "@adonisjs/mrm-preset": "^5.0.3",
    "@adonisjs/require-ts": "^2.0.13",
    "@types/node": "^18.8.4",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.0.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "japa": "^4.0.0",
    "japa-cli": "^1.0.1",
    "mrm": "^4.1.6",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4",
    "@adonisjs/view": "^6.2.0"
  },
  "peerDependencies": {
    "@adonisjs/auth": "^8.0.10",
    "@adonisjs/core": "^5.4.2",
    "@adonisjs/http-server": "^5.11.0",
    "@adonisjs/lucid": "^18.2.0",
    "@adonisjs/view": "^6.2.0"
  },
  "mrmConfig": {
    "core": false,
    "license": "MIT",
    "services": [
      "circleci"
    ],
    "minNodeVersion": "12.0.0",
    "probotApps": [
      "stale"
    ]
  },
  "files": [
    "build/src",
    "build/providers",
    "build/adonis-typings",
    "build/commands",
    "build/middleware",
    "build/ressources",
    "build/templates",
    "build/Exceptions",
    "build/index.d.ts",
    "build/index.js"
  ],
  "types": "build/adonis-typings",
  "typings": "build/adonis-typings",
  "adonisjs": {
    "instructionsMd": "./build/instructions.md",
    "types": "@fickou/adonis-access-control-list",
    "providers": [
      "@fickou/adonis-access-control-list"
    ],
    "templates": {
      "config": [
        "acl.txt"
      ]
    }
  }
}
